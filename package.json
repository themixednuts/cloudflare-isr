    "cf:types": "wrangler types cloudflare.d.ts"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.12.8",
    "@types/node": "^25.1.0",
    "typescript": "^5.9.3",
    "vite": "6",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "3.2.0"
  }
}
